{
  "name": "@prettyprinter-ts/core",
  "version": "0.0.1",
  "license": "MIT",
  "repository": "https://github.com/IMax153/prettyprinter-ts/core.git",
  "homepage": "https://github.com/IMax153/prettyprinter-ts",
  "author": "Maxwell Brown <maxwellbrown1990@gmail.com> (https://github.com/IMax153)",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "build:lib": "tsc -b tsconfig.build.json",
    "build:es6": "tsc -b tsconfig.build-es6.json",
    "build": "yarn run build:lib && yarn run build:es6",
    "clean": "rimraf build tsbuildinfo",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "demo": "ts-node -r tsconfig-paths/register"
  },
  "dependencies": {
    "@effect-ts/core": "^0.27.4",
    "@effect-ts/monocle": "^0.24.2",
    "@effect-ts/morphic": "^0.25.0",
    "@effect-ts/system": "^0.24.4",
    "@effect-ts/tracing-utils": "^0.5.0",
    "fast-check": "^2.14.0",
    "fast-equals": "^2.0.0"
  },
  "devDependencies": {}
}
