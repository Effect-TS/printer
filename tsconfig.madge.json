{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@effect/printer": ["./packages/printer/build/esm/index.js"],
      "@effect/printer/test/*": ["./packages/printer/build/test/*"],
      "@effect/printer/examples/*": ["./packages/printer/build/examples/*"],
      "@effect/printer/*": ["./packages/printer/build/esm/*"],
      "@effect/printer-ansi": ["./packages/printer-ansi/build/esm/index.js"],
      "@effect/printer-ansi/test/*": ["./packages/printer-ansi/build/test/*"],
      "@effect/printer-ansi/examples/*": [
        "./packages/printer-ansi/build/examples/*"
      ],
      "@effect/printer-ansi/*": ["./packages/printer-ansi/build/esm/*"]
    }
  },
  "include": [
    "./packages/printer/build/esm/**/*.js",
    "./packages/printer/build/test/**/*.js",
    "./packages/printer/build/examples/**/*.js",
    "./packages/printer-ansi/build/esm/**/*.js",
    "./packages/printer-ansi/build/test/**/*.js",
    "./packages/printer-ansi/build/examples/**/*.js"
  ]
}
